<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd"> 
	   
	 
	   
	   <bean id="collectionExample" class="com.Agilesoft.SpringCollectionExample">
	   		<property name="listOfNames">
	   			<list>
	   				<value>Amit</value>
	   				<value>Sumit</value>
	   				<value>Nilesh</value>
	   			</list>
	   		</property>	   	
	   		
	   		<property name="map">
	   			<map>
	   				<!-- key=rollNo, value="name" -->
	   				<entry key="1" value="Anil"></entry>
	   				<entry key="2" value="Sunil"></entry>
	   				<entry key="3" value="Shubham"></entry>
	   			</map>
	   		</property>
	   </bean>
  
</beans>